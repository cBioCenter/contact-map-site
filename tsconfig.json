{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "downlevelIteration": true,
    "jsx": "react",
    "lib": ["dom", "es2015", "es2016", "es2017"],
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./dist/",
    "paths": {
      "plotly.js/*": ["node_modules/@types/plotly.js"],
      "~contact-map-site~": ["./src"],
      "~contact-map-site~/test": ["./test"],
      "~contact-map-site~/*": ["./src/*"],
      "jest": ["node_modules/@types/jest/index.d.ts"]
    },
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "typeRoots": ["./types", "./node_modules/@types"]
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*", "types", "configs/**/*", "test/**/*", "./*.tsx"],
  "typeAcquisition": {
    "enable": false
  }
}
